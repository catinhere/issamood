{
    "version": 2,
    "functions": {
        "server/index.js": {
            "runtime": "nodejs12.x",
            "memory": 1024
        }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/index.js"
      }
    ],
    "outputDirectory": "client/build",
    "buildCommand": "cd server && npm install && cd .. && cd client && npm install && npm run build"
  }