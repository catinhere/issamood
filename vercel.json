{
    "version": 2,
    "builds": [
        {
          "src": "server/index.js",
          "use": "@vercel/node"
        }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/index.js"
      }
    ],
    "outputDirectory": "client/build",
    "buildCommand": "cd server && npm install && cd .. && cd client && npm install && npm run build"
  }